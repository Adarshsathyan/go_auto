<div class="header bg-primary pb-6">
      <div class="container-fluid">
        <div class="header-body">
          <div class="row align-items-center py-4">
            <div class="col-lg-6 col-7">
              <h6 class="h2 text-white d-inline-block mb-0">Auto Status</h6>
              <nav aria-label="breadcrumb" class="d-none d-md-inline-block ml-md-4">
                <ol class="breadcrumb breadcrumb-links breadcrumb-dark">
                  <li class="breadcrumb-item"><a href="#"><i class="fas fa-home"></i></a></li>
                  <li class="breadcrumb-item"><a href="#">Live status</a></li>
                </ol>
              </nav>
            </div>
            
          </div>
        </div>
      </div>
    </div>
<div class="container-fluid mt--6">
    <div class="row">
        <div class="col">
            <div class="card">
                <!-- Card header -->
                <div class="card-header border-0">
                    <h3 class="mb-0">Autos</h3>
                </div>
                <div class="table-responsive">
                    <table class="table align-items-center table-flush" id="data_tables">
                        <thead class="thead-light">
                            <tr>
                                <th scope="col" class="sort" data-sort="name">Drivers Name</th>
                                <th scope="col" class="sort" data-sort="budget">Reg Number</th>
                                <th scope="col" class="sort" data-sort="status">Status</th>
                                <th scope="col">Location</th>
                                
                            </tr>
                        </thead>
                        <tbody class="list">
                            {{#each autos}}
                            <tr>
                                <th scope="row">
                                    <div class="media align-items-center">
                                        <div class="media-body">
                                            <span class="name mb-0 text-sm">{{this.name}}</span>
                                        </div>
                                    </div>
                                </th>
                                <td class="budget">
                                    {{this.regno}}
                                </td>
                                <td>
                                    <span class="badge badge-dot mr-4">
                                        <i class="bg-warning"></i>
                                        <span class="status">{{this.drive}}</span>
                                    </span>
                                </td>
                                <td>
                                    {{this.location}}
                                </td>
                                
                            </tr>
                            {{/each}}
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
